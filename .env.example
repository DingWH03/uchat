DATABASE_URL=mysql://username:password@hostname:3306/database_name
DB_TYPE=mysql
SERVER_ADDRESS=0.0.0.0:25597

# DATABASE_URL=postgresql://username:password@hostname:5432/database_name
# DB_TYPE=postgres
# SERVER_ADDRESS=0.0.0.0:25597

REDIS_URL=redis://:your_redis_password@redis_host:6379/0

# MinIO 服务器地址和端口（HTTP 协议）
MINIO_ENDPOINT=http://minio_host:25603
# MinIO 访问密钥和密码（AccessKey 和 SecretKey）
MINIO_ACCESS_KEY=minio_key_example
MINIO_SECRET_KEY=minio_secret_example
# MinIO 存储桶名称
MINIO_BUCKET=uchat
# 公共访问 URL（一般是 MinIO 的地址加端口）
MINIO_BASE_URL=http://minio_host:25603

# 本地存储路径（如果你支持本地存储，留着备用）
LOCAL_STORAGE_DIR=./data
